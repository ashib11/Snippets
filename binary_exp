#include<iostream>
using namespace std;
int main()
{
int n,i=0,j=0;;
char str[1005];
cin>>n>>str;
while(str[i]=='.') i++;
while(n>0 && str[n-1]=='.') str[n--]='L';
while(str[j]!='L') j++;
cout<<i+1<<" "<<j<<endl;
}
#include <bits/stdc++.h>
using namespace std;
 int main ()
{
  long long n,c=0,m;
  cin>>n;
  map <long long,long long>mp;
  while (n--)
  {
    cin>>m;
    for (int i=0;i<=30;i++)
      c+=mp[(1<<i)-m];
    mp[m]++;

  }
  cout<<c<<"\n";
}

		 	 	 			   	 	  	  		 		 	