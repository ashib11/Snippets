#include<iostream>
using namespace std;
int main()
{
int n,i=0,j=0;;
char str[1005];
cin>>n>>str;
while(str[i]=='.') i++;
while(n>0 && str[n-1]=='.') str[n--]='L';
while(str[j]!='L') j++;
cout<<i+1<<" "<<j<<endl;
}
#include <bits/stdc++.h>
using namespace std;
 int main ()
{
  long long n,c=0,m;
  cin>>n;
  map <long long,long long>mp;
  while (n--)
  {
    cin>>m;
    for (int i=0;i<=30;i++)
      c+=mp[(1<<i)-m];
    mp[m]++;

  }
  cout<<c<<"\n";
}
#include<bits/stdc++.h>
using namespace std;
int a[30][100010];
int f[30];
int main(){
	int k=0,mmin=1e6;
	string s;
	cin>>s;
	for(int i=0;i<s.size();i++){
		k=s[i]-'a';
		a[k][++f[k]]=i+1;
	}
	for(int i=0;i<26;i++){
		int mmax=0;
		if(f[i]>=1) mmax=fmax(a[i][1],s.size()-a[i][f[i]]+1);
		else continue;
		for(int j=2;j<=f[i];j++){
			mmax=fmax(mmax,a[i][j]-a[i][j-1]);
		}
		mmin=min(mmax,mmin);
	}
	cout<<mmin;
	return 0;
}
  			    			 	   				   					 	
		 	 	 #include<bits/stdc++.h>
using namespace std;
typedef unsigned long long ull;
int main()
{
	ull n,k;

	cin>>n>>k;
	int bit=0;
	while(n>>bit) bit++;
	for(int i=bit-1,cnt=0;i>=0&&k>1;i--){
		if(((n>>i)&1)==0) n+=(ull)1<<i;
	}	
	cout<<n<<endl;
	
}			   	 	  	  		 		 	